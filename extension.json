{
	"name": "DarkMode",
	"version": "0.0.1",
	"author": [
		"MusikAnimal"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:DarkMode",
	"descriptionmsg": "darkmode-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
